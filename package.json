{
  "name": "vue-starter-config",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.18",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "sass": "^1.54.9",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.9.3",
    "unplugin-auto-import": "^0.11.2",
    "vite": "^3.0.9",
    "vitest": "^0.26.2",
    "vue-tsc": "^1.0.3"
  }
}
